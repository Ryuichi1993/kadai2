<h1>Editing Book</h1>
<%= form_for(@book) do |f| %>
<div class="Title">
	<p>Title</p>
		<%= f.text_field :title %>
</div>
<div class="Body">
	<p>Body</p>
		<%= f.text_area :body %>
</div>
<div class="cb">
	<%= f.submit 'Update Book' %>
</div>
<% end %>
<div class="icon">
	<%= link_to 'Show', book_path(@book) %>
</div>
<div class="icon">&nbsp|&nbsp
</div>
<div class="icon">
<%= link_to 'Back', books_path %>
</div>